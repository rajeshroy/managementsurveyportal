<mvc:View
	controllerName="mana.survey.portal.controller.Worklist"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:semantic="sap.m.semantic"
	xmlns:footerbar="sap.ushell.ui.footerbar">

	<semantic:FullscreenPage
		id="page"
        semanticRuleSet="Optimized"
		title="{i18n>worklistViewTitle}">
		<semantic:content>
			
			
			<Select id="managCustID"
					forceSelection="false"
					selectedKey="{/CustomerDetSet/0/kunnr}"
					items="{
						path: '/CustomerDetSet',
						sorter: { path: 'Name1' }
					}">
					<core:Item key="{Kunnr}" text="{Name1}" />
				</Select>
			<Button text="{i18n>worklistGo}" press="onGoPress"/>
		</semantic:content>
	</semantic:FullscreenPage>

</mvc:View>