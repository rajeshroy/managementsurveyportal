<mvc:View controllerName="mana.survey.portal.controller.Worklist" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core"
	xmlns:footerbar="sap.ushell.ui.footerbar" xmlns:f="sap.ui.layout.form">
	<FlexBox alignItems="End" justifyContent="Center">
		<items>
			<f:SimpleForm id="SimpleFormChange471" minWidth="1024" maxContainerCols="1" editable="true" layout="ResponsiveGridLayout"
				title="Please select customer:" labelSpanL="4" labelSpanM="4" emptySpanL="1" emptySpanM="1" columnsL="1" columnsM="1" class="editableForm">
				<f:content>
					<Select id="managCustID" forceSelection="false" selectedKey="{/CustomerDetSet/2/kunnr}"
						items="{ path: '/CustomerDetSet'}">
							<core:Item key="{Kunnr}" text="{Name1}"/>
					</Select>
					<Button text="{i18n>worklistGo}" press="onGoPress"/>
				</f:content>
			</f:SimpleForm>
		</items>
	</FlexBox>
</mvc:View>