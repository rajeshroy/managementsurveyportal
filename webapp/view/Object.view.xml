<mvc:View controllerName="mana.survey.portal.controller.Object" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" xmlns:viz="sap.viz.ui5.controls" xmlns:l="sap.ui.layout"
	xmlns:footerbar="sap.ushell.ui.footerbar">
	
<App>
	<Page id="page" navButtonPress="onNavBack" showNavButton="true"  title="{i18n>objectTitle}" busy="{objectView>/busy}"
		busyIndicatorDelay="{objectView>/delay}">

		<content>
			<ObjectHeader id="objectHeader" title="{Land1}"></ObjectHeader>

			<viz:VizFrame height="70%"  width="100%" xmlns="sap.viz" id="idpiechart" vizType="pie" selectData="onPress"/>
</content>
		<footer>
		</footer>
	</Page>
	
</App>
</mvc:View>

